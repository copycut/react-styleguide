{
  "name": "react-styleguide",
  "description": "React- Style Guide",
  "version": "0.0.1",
  "author": "Didier Chartrain",
  "license": "MIT",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "start": "styleguidist server",
    "build:documentation": "styleguidist build",
    "build:package": "cross-env NODE_ENV=production webpack -p",
    "test": "cross-env NODE_ENV=test npm run test:unit",
    "test:lint": "eslint src/**/*.js/ tests/**/*.js",
    "test:unit": "cross-env LOG_LEVEL=fatal mochapack --require esm --require tests/setup.js --webpack-config webpack-config.test.js src/**/*.spec.js"
  },
  "engines": {
    "node": ">=10"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "autoprefixer": "9.7.5",
    "babel-eslint": "^10.0.3",
    "browserlist": "^1.0.1",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.0",
    "dog-names": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "esm": "^3.2.25",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.2.1",
    "lodash": "^4.17.19",
    "lodash-webpack-plugin": "^0.11.5",
    "mocha": "^7.1.1",
    "mochapack": "^1.1.13",
    "node-sass": "^4.13.0",
    "nyc": "^15.0.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react": "^16.13.1",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.13.1",
    "react-styleguidist": "^11.0.4",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.0",
    "sinon": "^9.0.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.7.7",
    "babel-loader": "^8.0.6",
    "moment": "^2.24.0"
  }
}
